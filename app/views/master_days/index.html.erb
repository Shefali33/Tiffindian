<p id="notice"><%= notice %></p>

<h1>Master Days</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @master_days.each do |master_day| %>
      <tr>
        <td><%= link_to 'Show', master_day %></td>
        <td><%= link_to 'Edit', edit_master_day_path(master_day) %></td>
        <td><%= link_to 'Destroy', master_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Day', new_master_day_path %>
