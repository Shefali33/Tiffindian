<p id="notice"><%= notice %></p>

<h1>Master Mealtypes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @master_mealtypes.each do |master_mealtype| %>
      <tr>
        <td><%= link_to 'Show', master_mealtype %></td>
        <td><%= link_to 'Edit', edit_master_mealtype_path(master_mealtype) %></td>
        <td><%= link_to 'Destroy', master_mealtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Mealtype', new_master_mealtype_path %>
